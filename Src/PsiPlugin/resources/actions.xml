<?xml version="1.0" encoding="utf-8" ?> 
<actions>
  <action id="GotoGeneratedClass" text="Go to generated class"/>
  <action id="GotoGeneratedInterface" text="Go to generated interface"/>
  <action id="GotoGeneratedMethod" text="Go to generated method"/>

  <insert group-id="ReSharper.Navigate" position="last">
    <action-ref id="GotoGeneratedClass" />
  </insert>

  <insert group-id="ReSharper.Navigate" position="last">
    <action-ref id="GotoGeneratedInterface" />
  </insert>

  <insert group-id="ReSharper.Navigate" position="last">
    <action-ref id="GotoGeneratedMethod" />
  </insert>

  <insert group-id="VS#Code Window" position="last">
    <action-group id="PsiPlugin" text="PSI plugin" shared="true">
      <action-ref id="GotoGeneratedClass" />
      <action-ref id="GotoGeneratedInterface" />
      <action-ref id="GotoGeneratedMethod" />
    </action-group>
  </insert>
</actions>